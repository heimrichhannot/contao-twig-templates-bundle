{
  "name": "heimrichhannot/contao-twig-templates-bundle",
  "type": "contao-bundle",
  "description": "This bundle ships core contao templates as twig templates and additional templates using the latest Bootstrap framework's CSS for the Contao CMS.",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^7.4 || ^8.0",
    "contao/core-bundle": "^4.9",
    "heimrichhannot/contao-twig-support-bundle": "^0.2.1||^1.0",
    "heimrichhannot/contao-utils-bundle": "^2.222",
    "symfony/event-dispatcher": "^4.4||^5.4",
    "symfony/event-dispatcher-contracts": "^1||^2||^3",
    "symfony/translation": "^4.4||^5.4",
    "symfony/translation-contracts": "^1.1.6||^2||^3",
    "symfony/polyfill-php80": "^1.26"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\TwigTemplatesBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\TwigTemplatesBundle\\ContaoManager\\Plugin"
  }
}
